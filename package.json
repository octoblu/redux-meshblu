{
  "name": "redux-meshblu",
  "version": "1.0.5",
  "description": "Redux Actions with a dose of Meshblu.",
  "main": "dist/bundle.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.js",
    "build": "npm run clean && npm run build:webpack",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "nyc --require babel-core/register --require babel-polyfill mocha",
    "test:watch": "mocha -w -R mocha-multi --reporter-options spec=-,mocha-osx-reporter=-",
    "coverage": "nyc report --reporter=lcov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octoblu/redux-meshblu.git"
  },
  "author": "Octoblu Inc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/octoblu/redux-meshblu/issues"
  },
  "homepage": "https://github.com/octoblu/redux-meshblu#readme",
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "coffee-script": "^1.10.0",
    "eslint": "^3.1.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.12.0",
    "mocha": "^2.5.3",
    "mocha-multi": "^0.9.0",
    "mocha-osx-reporter": "^0.1.2",
    "npm-install-webpack-plugin": "^4.0.4",
    "nyc": "^7.1.0",
    "redux-mock-store": "^1.1.2",
    "server-destroy": "^1.0.1",
    "shmock": "^0.8.1",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "browser-meshblu-http": "^5.2.9",
    "redux": "^3.5.2",
    "redux-act": "^0.5.0",
    "redux-thunk": "^2.1.0"
  }
}
